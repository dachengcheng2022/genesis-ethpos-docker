[program:{{SYMBOL}}]
autorestart=true
user={{SYMBOL}}
stdout_logfile=/data-ephemeral/{{SYMBOL}}.log
stdout_logfile_maxbytes=104857600
redirect_stderr=true
command=/usr/local/bin/start_eth.sh
priority=20

[program:{{SYMBOL}}-stdout-tail]
autorestart=true
user={{SYMBOL}}
stdout_logfile=/dev/stdout
stdout_logfile_maxbytes=0
stderr_logfile=/dev/stderr
stderr_logfile_maxbytes=0
command=tail -F /data-ephemeral/{{SYMBOL}}.log
priority=21

[program:healthcheck-tail]
autostart=true
autorestart=true
user={{SYMBOL}}
stdout_logfile=/dev/stdout
stdout_logfile_maxbytes=0
redirect_stderr=true
command=tail -F /data-ephemeral/healthcheck.log
priority=99

[program:dd-metrics]
autostart=true
autorestart=true
startretries=60
user={{SYMBOL}}
stdout_logfile=/dev/stdout
stdout_logfile_maxbytes=0
redirect_stderr=true
command=/dd-metrics.sh
priority=99
